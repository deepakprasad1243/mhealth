<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jack.brainwaves" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.NoActionBar">
        <activity
            android:name=".SplashScreenActivity"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Light.NoTitleBar.Fullscreen"
            android:label="@string/title_activity_splash_screen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar">
        </activity>
        <activity
            android:name=".LogXYPlotActivity"
            android:label="@string/title_activity_simple_xyplot" >
        </activity>
        <activity
            android:name=".DynamicXYPlotActivity"
            android:label="@string/title_activity_dynamic_xyplot" >
        </activity>
        <activity
            android:name=".RealtimePlotActivity"
            android:label="@string/title_activity_orientation_sensor_example" >
        </activity>
        <activity
            android:name=".MuseActivity"
            android:label="@string/title_activity_muse" >
        </activity>
    </application>

</manifest>
