<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jack.brainwaves" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jack.brainwaves.MainActivity" />
        </activity>
        <activity
            android:name=".Brainwaves"
            android:label="@string/title_activity_brainwaves" >
        </activity>
        <activity
            android:name=".SimpleXYPlotActivity"
            android:label="@string/title_activity_simple_xyplot" >
        </activity>
        <activity
            android:name=".DynamicXYPlotActivity"
            android:label="@string/title_activity_dynamic_xyplot" >
        </activity>
        <activity
            android:name=".OrientationSensorExampleActivity"
            android:label="@string/title_activity_orientation_sensor_example" >
        </activity>
        <activity
            android:name=".MuseActivity"
            android:label="@string/title_activity_muse" >
        </activity>
        <activity
            android:name=".SplashScreenActivity"
            android:label="@string/title_activity_splash_screen" >
        </activity>
    </application>

</manifest>
